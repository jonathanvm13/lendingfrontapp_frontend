body
  height: 100%;
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;

section
  display: flex !important
  justify-content: center

header
  width: 100%
  display: flex !important
  height: 84px
  justify-content: center
  position: fixed
  background-color: white

.container
  width: 100%

.center-self
  align-self: center

.response
  font-size: 40px
  margin-top: 100px

.form-container
  display: flex
  flex-direction: column
  margin-top: 84px
  position: absolute
  width: 100%
  left: 0px
  height: calc(100% - 84px)
  overflow-y: auto

.loader {
  margin: 100px auto;
  font-size: 25px;
  width: 1em;
  height: 1em;
  border-radius: 50%;
  position: relative;
  text-indent: -9999em;
  -webkit-animation: load5 1.1s infinite ease;
  animation: load5 1.1s infinite ease;
  -webkit-transform: translateZ(0);
  -ms-transform: translateZ(0);
  transform: translateZ(0);
}

